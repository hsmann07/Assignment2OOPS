<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="541.0" prefWidth="654.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111" fx:controller="car.CarRegistrationController">
   <children>
      <Label layoutX="10.0" layoutY="14.0" text="CAR REGISTRATION FORM" textFill="#ad2a2a">
         <font>
            <Font name="System Bold Italic" size="48.0" />
         </font>
      </Label>
      <HBox layoutX="4.0" layoutY="84.0" prefHeight="237.0" prefWidth="650.0">
         <children>
            <GridPane hgap="10.0" prefHeight="178.0" prefWidth="468.0" vgap="10.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="MAKE" textFill="#c61111" />
                  <Label text="MODEL" textFill="#c61111" GridPane.rowIndex="1" />
                  <Label text="YEAR" textFill="#c61111" GridPane.rowIndex="2" />
                  <Label text="PRICE" textFill="#c61111" GridPane.rowIndex="3" />
                  <Label text="MILEAGE" textFill="#c61111" GridPane.rowIndex="4" />
                  <TextField fx:id="make" GridPane.columnIndex="1" />
                  <TextField fx:id="model" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <TextField fx:id="year" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <TextField fx:id="price" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <TextField fx:id="mileage" GridPane.columnIndex="1" GridPane.rowIndex="4" />
               </children>
            </GridPane>
         </children>
      </HBox>
      <Button fx:id="register" layoutX="534.0" layoutY="406.0" mnemonicParsing="false" onAction="#registerButtonPushed" text="Register" textFill="#1836ae" />
      <Label fx:id="warningLabel" layoutX="28.0" layoutY="450.0" prefHeight="49.0" prefWidth="562.0" text="Label" />
   </children>
</AnchorPane>
